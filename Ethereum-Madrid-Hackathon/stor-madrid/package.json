  },
  "homepage": "https://github.com/energywebfoundation/ew-origin#readme",
  "dependencies": {
    "ethereumjs-tx": "^1.3.4",
    "ganache-cli": "^6.1.0",
    "solidity-coverage": "^0.4.9",
    "truffle": "^4.1.0",
    "typescript": "^2.7.1",
    "web3": "1.0.0-beta.33",
    "webpack-dev-server": "^3.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "9.4.5",
    "chai": "^4.1.2",
    "mocha": "^5.0.1"
  }
}
